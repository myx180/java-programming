public class Lab2Task22 {
    public static void main(String[] args) {
        int x = 1, a = 3;

        switch (a) {
            case 1:
                x += 5;
                break;
            case 2:
                x += 10;
                break;
            case 3:
                x += 16;
                break;
            case 4:
                x += 34;
                break;
            default:
                // Do nothing or handle other cases if needed
                break;
        }

        System.out.println("Value of x: " + x); // This will print the value of x after the switch statement
    }
}
